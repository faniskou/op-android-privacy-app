  "definitions": {
    "StatisticPolicy": {
      "type": "object",
      "required": [
        "name",
        "domainurl",
        "count"
      ],
      "properties": {
        "id": {
          "type": "integer",
          "format": "int64"
        },
        "name": {
          "type": "string",
          "example": "policyA"
        },
        "domainurl": {
          "type": "string",
          "example": "www.enikos.gr"
        },
        "count": {
          "type": "integer",
          "format": "int64",
          "example": 2
        },
        "modified": {
          "type": "string",
          "example": "2001.07.04 AD at 12:08:56 PDT"
        },
        "sourceactivity": {
          "type": "string",
          "example": "operando"
        },
        "hidden": {
          "type": "string",
          "example": "no"
        },
        "category": {
          "type": "string",
          "example": "news"
        },
        "extrainfo": {
          "type": "string",
          "example": "{'for initialatation':'yes'}"
        }
      }
    },
    "AppIdentityGroup": {
      "type": "object",
      "required": [
        "name",
        "app_name",
        "count"
      ],
      "properties": {
        "id": {
          "type": "integer",
          "format": "int64"
        },
        "name": {
          "type": "string",
          "example": "initialgroup"
        },
        "domainurl": {
          "type": "string",
          "example": "www.enikos.gr"
        },
        "app_name": {
          "type": "string",
          "example": "ClashOfClanshGame"
        },
        "count": {
          "type": "integer",
          "format": "int64",
          "example": 2
        },
        "duration": {
          "type": "integer",
          "format": "int64",
          "example": 21
        }
      }
    },
    "AppIdentityGroups": {
      "type": "object",
      "properties": {
        "apps": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string",
                "example": "initialgroup"
              }
            }
          }
        }
      }
    }
